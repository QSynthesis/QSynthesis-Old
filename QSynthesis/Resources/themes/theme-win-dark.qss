ComboSelector {
    background-color: transparent;
    qproperty-wallpaper: #252526;
    /* border-top: 10px solid qlineargradient(y0:0, y1:1, stop: 0 #ECECEC, stop: 1 transparent); */
    border-top: none;
    border-left: 8px solid qlineargradient(x0:0, x1:1, stop: 0 #111111, stop: 1 transparent);
    border-bottom: 8px solid qlineargradient(y0:0, y1:1, stop: 0 transparent, stop: 1 #111111);
    border-right: 8px solid qlineargradient(x0:0, x1:1, stop: 0 transparent, stop: 1 #111111);
}

ComboSelector>FixedLineEdit {
    font-size: 15px;
    padding: 3px 3px;
}

ComboSelector>QListWidget {
    border: none;
    color: #CCCCCC;
    background-color: transparent;
}

ComboSelector>QListWidget::item {
    font-size: 15px;
    padding: 3px 3px;
}

/* QScrollBar */

QScrollBar:vertical {
    background: transparent;
    width: 8px;
    margin: 0px 0px 0px 0px;
}

QScrollBar::handle:vertical {
    background: rgba(223, 223, 225, 200);
    border: 0px;
    border-radius: 4px;
    margin: 0px 0px 0px 0px;
    min-height: 40px;
}

QScrollBar::handle:vertical:hover {
    background: lightgray;
}

QScrollBar::handle:vertical:pressed {
    background: rgba(200, 200, 200, 255);
}

QScrollBar::sub-page:vertical {
    background: transparent;
}

QScrollBar::add-page:vertical {
    background: transparent;
}

QScrollBar::up-arrow {
    background: transparent;
}

QScrollBar::down-arrow {
    background: transparent;
}

QScrollBar::sub-line:vertical {
    background: transparent;
    width: 0px;
}

QScrollBar::add-line:vertical {
    background: transparent;
    width: 0px;
}

QScrollBar:horizontal {
    background: transparent;
    height: 8px;
    margin: 0px 0px 0px 0px;
}

QScrollBar::handle:horizontal {
    background: rgba(223, 223, 225, 200);
    border: 0px;
    border-radius: 4px;
    margin: 0px 0px 0px 0px;
    min-width: 40px;
}

QScrollBar::handle:horizontal:hover {
    background: lightgray;
}

QScrollBar::handle:horizontal:pressed {
    background: rgba(200, 200, 200, 255);
}

QScrollBar::sub-page:horizontal {
    background: transparent;
}

QScrollBar::add-page:horizontal {
    background: transparent;
}

QScrollBar::left-arrow {
    background: transparent;
}

QScrollBar::right-arrow {
    background: transparent;
}

QScrollBar::sub-line:horizontal {
    background: transparent;
    height: 0px;
}

QScrollBar::add-line:horizontal {
    background: transparent;
    height: 0px;
}