ComboSelector {
    background-color: transparent;
    qproperty-wallpaper: #F3F3F3;

    /* border-top: 10px solid qlineargradient(y0:0, y1:1, stop: 0 #ECECEC, stop: 1 transparent); */
    border-top: none;
    border-left: 8px solid qlineargradient(x0:0, x1:1, stop: 0 #ECECEC, stop: 1 transparent);
    border-bottom: 8px solid qlineargradient(y0:0, y1:1, stop: 0 transparent, stop: 1 #ECECEC);
    border-right: 8px solid qlineargradient(x0:0, x1:1, stop: 0 transparent, stop: 1 #ECECEC);
}

ComboSelector>FixedLineEdit {
    font-size: 15px;
    padding: 3px 3px;
}

ComboSelector>QListWidget {
    border: none;
    color: black;
    background-color: transparent;
}

ComboSelector>QListWidget::item {
    font-size: 15px;
    padding: 3px 3px;
}

/* QScrollBar */

QScrollBar {
    background: transparent;
    margin: 0px 0px 0px 0px;
}

QScrollBar:vertical {
    width: 10px;
}

QScrollBar:horizontal {
    height: 10px;
}

QScrollBar::handle {
    background: rgba(0, 0, 0, 15%);
    border: 0px;
    border-radius: 3px;
    margin: 0px 0px 0px 0px;
}

QScrollBar::handle:vertical {
    min-height: 40px;
}

QScrollBar::handle:horizontal {
    min-width: 40px;
}

QScrollBar::handle:hover {
    background: rgba(0, 0, 0, 20%);
}

QScrollBar::handle:pressed {
    background: rgba(0, 0, 0, 18%);
}

QScrollBar::sub-page {
    background: transparent;
}

QScrollBar::add-page {
    background: transparent;
}

QScrollBar::up-arrow {
    background: transparent;
}

QScrollBar::down-arrow {
    background: transparent;
}

QScrollBar::left-arrow {
    background: transparent;
}

QScrollBar::right-arrow {
    background: transparent;
}

QScrollBar::sub-line {
    background: transparent;
    width: 0px;
}

QScrollBar::add-line {
    background: transparent;
    width: 0px;
}

/* VectorButton */

VectorButton {
    qproperty-upColor: darkgrey;
    qproperty-overColor: lightgrey;
    qproperty-downColor: darkgrey;
}

/* QMessageBox */

QMessageBox {
    background-color: whitesmoke;
    border-radius: 3px;
}