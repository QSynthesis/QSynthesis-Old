/* TracksInterface */

TracksInterface>QFrame[type=track-separator] {
    border: none;
    background: #121212;
}

/* TrackHead */

HeadsArea {
    background-color: transparent;
}

TrackHead {
    background-color: #333333;
    qproperty-iconColor: lightgrey;
}

TrackHead>FixedLineEdit {
    border: none;
    background-color: transparent;
    padding: 1px;
    color: #D7D7D7;
}

TrackHead>FixedLineEdit:focus {
    background-color: rgba(255, 255, 255, 5%);
}

TrackHead>LabelButton {
    border: none;
    background: transparent;
    text-align: left;
    padding: 0 3px;
    color: #999999;
}

TrackHead>LabelButton[invalid=true] {
    color: orangered;
}

TrackHead>LabelButton[invalid=false]::hover {
    color: white;
}

TrackHead>LabelButton[invalid=true]::hover {
    color: red;
}

TrackHead>TrackButton {
    border: 1px solid transparent;
    background-color: #0E639C;
    color: white;
    /* border-radius: 2px; */
}

TrackHead>TrackButton:disabled {
    color: #666666;
}

TrackHead>TrackButton:hover {
    background-color: #1177BB;
}

TrackHead>TrackButton:pressed {
    color: #EEEEEE;
}

/* TrackContent */

BlocksArea {
    background-color: transparent;
}

TrackContent {
    background-color: rgba(255, 255, 255, 10%);
    qproperty-midiColor: #00AEFF;
}

/* ParamsDialArea */

ParamsDialArea {
    qproperty-textColor: lightgrey;
}

/* BaseDialog */

BaseDialog {
    background-color: #252526;
}

/* QTableWidget Voice */

QTableWidget[type=voice] {
    background: #383838;
    alternate-background-color: #323232;
    gridline-color: rgba(255, 255, 255, 15%);
    selection-background-color: transparent;
}

/*items*/

QTableWidget[type=voice]::item {
    color: #DCDCDC;
    /* background-color: transparent; */
}

QTableWidget[type=voice]::item:hover {
    background-color: rgba(255, 255, 255, 15%);
}

QTableWidget[type=voice]::item:pressed {
    background-color: rgba(255, 255, 255, 7.5%);
}

QTableWidget[type=voice]::item:selected {
    background-color: rgba(255, 255, 255, 10%);
}

/*corner*/

QTableWidget[type=voice]>QTableCornerButton::section {
    background-color: #323232;
    border: 1px solid rgba(255, 255, 255, 5%);
}

QTableWidget[type=voice]>QTableCornerButton::section:pressed {
    background-color: rgba(255, 255, 255, 10%);
}

/*header*/

QTableWidget[type=voice]>QHeaderView {
    background-color: #323232;
}

QTableWidget[type=voice]>QHeaderView::section {
    background-color: transparent;
    padding: 3px;
    margin: 0px;
    color: #DCDCDC;
    border: 1px solid rgba(255, 255, 255, 5%);
}

QTableWidget[type=voice]>QHeaderView::section:horizontal {
    border-left-width: 0;
}

QTableWidget[type=voice]>QHeaderView::section:vertical {
    border-top-width: 0;
}

QTableWidget[type=voice]>QHeaderView::section:hover {
    background-color: rgba(255, 255, 255, 20%);
}

QTableWidget[type=voice]>QHeaderView::section:pressed {
    background-color: rgba(255, 255, 255, 10%);
}

QTableWidget[type=voice]>QHeaderView::section:checked {
    background-color: rgba(255, 255, 255, 15%);
}

/* QTableWidget Keyboard */

QTableWidget[type=keyboard] {
    border: none;
    background: #1E1E1E;
    alternate-background-color: #222222;
    gridline-color: rgba(255, 255, 255, 7.5%);
    selection-background-color: transparent;
}

/*items*/

QTableWidget[type=keyboard]::item {
    color: #DCDCDC;
    background-color: transparent;
}

QTableWidget[type=keyboard]::item:hover {
    background-color: rgba(255, 255, 255, 20%);
}

QTableWidget[type=keyboard]::item:pressed {
    background-color: rgba(255, 255, 255, 7.5%);
}

QTableWidget[type=keyboard]::item:selected {
    background-color: rgba(255, 255, 255, 12%);
}

/*corner*/

QTableWidget[type=keyboard]>QTableCornerButton::section {
    background-color: #222222;
    border: none;
}

QTableWidget[type=keyboard]>QTableCornerButton::section:pressed {
    background-color: rgba(255, 255, 255, 10%);
}

/*header*/

QTableWidget[type=keyboard]>QHeaderView {
    background-color: #222222;
}

QTableWidget[type=keyboard]>QHeaderView::section {
    background-color: transparent;
    padding: 3px;
    margin: 0px;
    color: #DCDCDC;
    border: none;
}

QTableWidget[type=keyboard]>QHeaderView::section:horizontal {
    font-weight: bold;
}

QTableWidget[type=keyboard]>QHeaderView::section:vertical:hover {
    background-color: rgba(255, 255, 255, 20%);
}

QTableWidget[type=keyboard]>QHeaderView::section:vertical:pressed {
    background-color: rgba(255, 255, 255, 10%);
}

QTableWidget[type=keyboard]>QHeaderView::section:vertical:checked {
    background-color: rgba(255, 255, 255, 13.6%);
}

/* ComboSelector */

ComboSelector {
    background-color: transparent;
    qproperty-wallpaper: #252526;
    /* border-top: 10px solid qlineargradient(y0:0, y1:1, stop: 0 #ECECEC, stop: 1 transparent); */
    border-top: none;
    border-left: 8px solid qlineargradient(x0:0, x1:1, stop: 0 #111111, stop: 1 transparent);
    border-bottom: 8px solid qlineargradient(y0:0, y1:1, stop: 0 transparent, stop: 1 #111111);
    border-right: 8px solid qlineargradient(x0:0, x1:1, stop: 0 transparent, stop: 1 #111111);
}

ComboSelector>FixedLineEdit {
    font-size: 15px;
    padding: 3px 3px;
}

ComboSelector>QListWidget {
    border: none;
    color: #CCCCCC;
    background-color: transparent;
}

ComboSelector>QListWidget::item {
    font-size: 15px;
    padding: 3px 3px;
}

/* QScrollBar */

QScrollBar {
    background: transparent;
    margin: 0px 0px 0px 0px;
}

QScrollBar:vertical {
    width: 10px;
}

QScrollBar:horizontal {
    height: 10px;
}

QScrollBar::handle {
    background: rgba(255, 255, 255, 15%);
    border: 0px;
    border-radius: 3px;
    margin: 0px 0px 0px 0px;
}

QScrollBar::handle:vertical {
    min-height: 40px;
}

QScrollBar::handle:horizontal {
    min-width: 40px;
}

QScrollBar::handle:hover {
    background: rgba(255, 255, 255, 20%);
}

QScrollBar::handle:pressed {
    background: rgba(255, 255, 255, 18%);
}

QScrollBar::sub-page {
    background: transparent;
}

QScrollBar::add-page {
    background: transparent;
}

QScrollBar::up-arrow {
    background: transparent;
}

QScrollBar::down-arrow {
    background: transparent;
}

QScrollBar::left-arrow {
    background: transparent;
}

QScrollBar::right-arrow {
    background: transparent;
}

QScrollBar::sub-line {
    background: transparent;
    width: 0px;
}

QScrollBar::add-line {
    background: transparent;
    width: 0px;
}

/* VectorButton */

VectorButton {
    qproperty-upColor: #CCCCCC;
    qproperty-overColor: white;
    qproperty-downColor: #DDDDDD;
}

VectorButton[type=tab-close][highlight=false] {
    qproperty-upColor: #808080;
    qproperty-overColor: #B0B0B0;
    qproperty-downColor: #9A9A9A;
}

VectorButton[type=tab-close][highlight=true] {
    qproperty-upColor: #DDDDDD;
    qproperty-overColor: white;
    qproperty-downColor: #FAFAFA;
}

/* QMessageBox */

QMessageBox {
    background-color: #252526;
    border-radius: 3px;
}

/* VoiceBankTab */

OtoInfoArea>LineControl>.QLabel {
    padding: 5px 5px;
}

OtoInfoArea>LineControl>FixedLineEdit {
    /* padding: 5px 5px; */
}