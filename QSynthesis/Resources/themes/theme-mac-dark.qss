/* General */

QWidget:focus {
    outline: none;
}

QScrollArea {
    background-color: transparent;
}

NoneScrollArea {
    background-color: transparent;
}

QGraphicsView {
    background-color: transparent;
}

.QLabel {
    color: #CCCCCC;
}

QCheckBox {
    color: #CCCCCC;
}

QLineEdit, QTextEdit, QPlainTextEdit, QComboBox {
    border: 1px solid transparent;
    color: #CCCCCC;
    background-color: #3C3C3C;
    padding: 3px 3px;
}

WaveformScrollArea, QTableWidget {
    border: 1px solid transparent;
    background-color: #3C3C3C;
}

QLineEdit:focus, QTextEdit:focus, QPlainTextEdit:focus, QComboBox:focus, WaveformScrollArea:focus, QTableWidget:focus {
    border-color: #167FD2;
}

.QPushButton {
    border: 1px solid transparent;
    background-color: #0E639C;
    color: white;
    /* border-radius: 2px; */
    padding: 5px auto;
}

.QPushButton:disabled {
    color: #666666;
}

.QPushButton:hover {
    background-color: #1177BB;
}

.QPushButton:pressed {
    color: #EEEEEE;
}

.SwitchButton {
    background-color: transparent;
    border: none;
}

/* Custom Controls */

LineControl>FixedLineEdit[status=normal] {
    background-color: #3C3C3C;
}

LineControl>FixedLineEdit[status=unmodified] {
    background-color: #666666;
}

ComboControl>FixedComboBox[status=normal] {
    background-color: #3C3C3C;
}

ComboControl>FixedComboBox[status=unmodified] {
    background-color: #666666;
}

SlideControl>FixedSpinBox[status=normal] {
    background-color: #3C3C3C;
}

SlideControl>FixedSpinBox[status=unmodified] {
    background-color: #666666;
}

SlideLineControl>FixedLineEdit[status=normal] {
    background-color: #3C3C3C;
}

SlideLineControl>FixedLineEdit[status=unmodified] {
    background-color: #666666;
}

/* TabWidget */

TabWidget::pane {
    border: 0;
}

TabWidget::tab-bar {
    left: 0;
}

/* Main TabWidget */

CentralTabWidget {
    background-color: #252526;
}

CentralTabWidget>QTabBar::tab {
    border-right: 1px solid #252526;
    border-left: 1px solid #252526;
    color: #A0A0A0;
    padding: 12px 15px;
    background-color: #2D2D2D;
}

CentralTabWidget>QTabBar::tab:selected {
    color: white;
    background-color: #1E1E1E;
}

CentralTabWidget>QTabBar::tab:!selected:hover {
    background-color: #333333;
}

CentralTabWidget>QTabBar::scroller {
    width: 40px;
}

/* Voice TabWidget */

TabWidget[type=voice] {
    background-color: transparent;
}

TabWidget[type=voice]>QTabBar::tab {
    border-top: 1px solid #404040;
    border-right: 1px solid #404040;
    border-left: 1px solid #404040;
    color: #CCCCCC;
    padding: 5px 8px;
    background-color: #2D2D2D;
}

TabWidget[type=voice]>QTabBar::tab:selected {
    color: white;
    background-color: #505050;
}

TabWidget[type=voice]>QTabBar::tab:!selected:hover {
    background-color: #383838;
}

TabWidget[type=voice]>QTabBar::scroller {
    width: 40px;
}

/* Keyboard TabWidget */

TabWidget[type=keyboard] {
    background-color: transparent;
}

TabWidget[type=keyboard]>QTabBar {
    border: none;
    font-size: 16px;
    /* border-bottom: 1px solid darkgrey; */
}

TabWidget[type=keyboard]>QTabBar::tab {
    background-color: transparent;
    border: 1px solid transparent;
    color: lightgrey;
    padding: 8px 8px;
    margin-bottom: 25px;
}

TabWidget[type=keyboard]>QTabBar::tab:first {
    margin-right: 10px;
}

TabWidget[type=keyboard]>QTabBar::tab:middle {
    margin-left: 10px;
    margin-right: 10px;
}

TabWidget[type=keyboard]>QTabBar::tab:last {
    margin-left: 10px;
}

TabWidget[type=keyboard]>QTabBar::tab:selected {
    color: white;
    border-bottom-color: white;
}

TabWidget[type=keyboard]>QTabBar::tab:!selected:hover {
    color: white;
}

TabWidget[type=keyboard]>QTabBar::scroller {
    width: 40px;
}

KeyboardTab>.SwitchButton[type=key-table-tab] {
    color: #898A8B;
    font-size: 17px;
    padding: 14px 8px;
}

KeyboardTab>.SwitchButton[type=key-table-tab]:hover {
    color: #DCDCDC;
}

KeyboardTab>.SwitchButton[type=key-table-tab]:checked {
    color: #DCDCDC;
}

KeyboardTab>.SwitchButtonUnderline[type=keyboard] {
    qproperty-lineWidth: 1;
    qproperty-color: #898A8B;
    background-color: #333333;
}

KeyboardTab>.QPushButton#reset {}

KeyModifyDialog {
    background-color: #252526;
}

/* Central Tab */

CentralTab {
    background-color: #1B1A19;
}

CentralTabContent {
    background-color: transparent;
}

/* Welcome Tab */

WelcomeTabContent>.SwitchButton[type=list] {
    color: #666666;
    font-size: 18px;
    padding: 10px 0;
}

WelcomeTabContent>.SwitchButton[type=list]:hover {
    color: #DBDBDB;
}

WelcomeTabContent>.SwitchButton[type=list]:checked {
    color: #DBDBDB;
}

WelcomeTabContent>.SwitchButtonUnderline[type=list] {
    qproperty-lineWidth: 4;
    qproperty-color: #0E639C;
}

WelcomeTabContent>.QPushButton[type=special] {
    padding: 8px 35px;
    font-size: 20px;
}

WelcomeTabContent>.FileListWidget {
    background-color: transparent;
    qproperty-itemHeight: 72;
    qproperty-selectColor: rgba(255, 255, 255, 10%);
    qproperty-underlineColor: #3A3938;
    qproperty-filenameColor: #E0E0E0;
    qproperty-directoryColor: #909090;
    qproperty-dateColor: #909090;
}

WelcomeTabContent>QLabel[type=title] {
    font-size: 30px;
    font-weight: bold;
}

/* Menu Bar 

QMenuBar {
    background: #3C3C3C;
    border-bottom: 1px solid rgba(255, 255, 255, 5%);
}

QMenuBar::item {
    color: #CCCCCC;
    padding: 8px 10px;
}

QMenuBar::item:selected {
    background: #505050;
}

QMenu {
    padding: 5px 5px;
    background: #252526;
}

QMenu::item {
    padding: 6px 30px;
    background: transparent;
    color: #CCCCCC;
}

QMenu::item:selected {
    color: white;
    background: #0060C0;
}

QMenu::item:disabled {
    color: #666666;
}

QMenu::separator {
    height: 1px;
    background-color: #5B5B5B;
    margin: 6px 6px 6px 6px;
}

/* Title Bar */

FormSplitter::handle {
    background-color: transparent;
}

FormTitleBar {
    qproperty-fillColor: #1E1E1E;
    qproperty-topColor: #333333;
    qproperty-top: 1;
    qproperty-contentHeight: 40;
}

.QSplitter[type=voice-splitter]::handle {
    background-color: transparent;
}

.QSplitter[type=voice-splitter]::handle:horizontal {
    width: 15px;
}

.QSplitter[type=voice-splitter]::handle:vertical {
    height: 15px;
}

.SwitchButton[type=titleBar] {
    color: #898A8B;
    border-bottom: 1px solid transparent;
    padding: 4px 0;
}

.SwitchButton[type=titleBar]:hover {
    color: #D7D7D7;
}

.SwitchButton[type=titleBar]:checked {
    color: #D7D7D7;
    border-bottom-color: white;
}

/* Editor */

SectionsArea {
    qproperty-noteTempoColor: #CCCCCC;
    qproperty-globalTempoColor: rgb(255, 129, 150);
    qproperty-beatColor: #CCCCCC;
    qproperty-numColor: #CCCCCC;
    qproperty-lineColor: #999999;
}

SectionsLine {
    qproperty-beatColor: lightgrey;
    qproperty-numColor: lightgrey;
    qproperty-lineColor: #999999;
}

PianoArea {
    background-color: transparent;
}

PianoArea>QLabel[type=standard] {
    color: #333333;
}

PianoArea>QLabel[type=realtime] {
    color: black;
    background-color: transparent;
}

PianoArea>QLabel[type=realtime][black=true] {
    color: white;
}

PianoKey {
    border: none;
    border-bottom-right-radius: 3px;
    border-top-right-radius: 3px;
    margin: 1px 1px 1px 0/* top right bottom left */
}

PianoKey[color=white] {
    background-color: lightgrey;
}

PianoKey[color=white]:hover {
    background-color: #4794CB;
}

PianoKey[color=black] {
    background-color: black;
}

PianoKey[color=black]:hover {
    background-color: #4794CB;
}

